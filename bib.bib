@article{whitaker_constraining_2014,
	title = {{CONSTRAINING} {THE} {LOW}-{MASS} {SLOPE} {OF} {THE} {STAR} {FORMATION} {SEQUENCE} {AT} 0.5 \${\textbackslash}less\$z\${\textbackslash}less\$ 2.5},
	volume = {795},
	issn = {0004-637X},
	url = {https://doi.org/10.1088%2F0004-637x%2F795%2F2%2F104},
	doi = {10.1088/0004-637X/795/2/104},
	abstract = {We constrain the slope of the star formation rate (SFR; log $\Psi$) to stellar mass (log M *) relation down to log (M */M ?) = 8.4 (log (M */M ?) = 9.2) at z = 0.5 (z = 2.5) with a mass-complete sample of 39,106 star-forming galaxies selected from the 3D-HST photometric catalogs, using deep photometry in the CANDELS fields. For the first time, we find that the slope is dependent on stellar mass, such that it is steeper at low masses (log $\Psi$?log M *) than at high masses (log $\Psi$?(0.3-0.6)log M *). These steeper low-mass slopes are found for three different star formation indicators: the combination of the ultraviolet (UV) and infrared (IR), calibrated from a stacking analysis of Spitzer/MIPS 24 $\mu$m imaging; $\beta$-corrected UV SFRs; and H$\alpha$ SFRs. The normalization of the sequence evolves differently in distinct mass regimes as well: for galaxies less massive than log (M */M ?) {\textless} 10 the specific SFR ($\Psi$/M *) is observed to be roughly self-similar with $\Psi$/M *?(1 + z)1.9, whereas more massive galaxies show a stronger evolution with $\Psi$/M *?(1 + z)2.2-3.5 for log (M */M ?) = 10.2-11.2. The fact that we find a steep slope of the star formation sequence for the lower mass galaxies will help reconcile theoretical galaxy formation models with the observations.},
	language = {en},
	number = {2},
	urldate = {2019-04-15},
	journal = {ApJ},
	author = {Whitaker, Katherine E. and Franx, Marijn and Leja, Joel and Dokkum, Pieter G. van and Henry, Alaina and Skelton, Rosalind E. and Fumagalli, Mattia and Momcheva, Ivelina G. and Brammer, Gabriel B. and Labb{\'e}, Ivo and Nelson, Erica J. and Rigby, Jane R.},
	month = oct,
	year = {2014},
	pages = {104}
}


@article{dave_simba:_2019,
	title = {{SIMBA}: {Cosmological} simulations with black hole growth and feedback},
	volume = {486},
	shorttitle = {{SIMBA}},
	url = {https://ui.adsabs.harvard.edu/abs/2019MNRAS.486.2827D/abstract},
	doi = {10.1093/mnras/stz937},
	abstract = {We introduce the SIMBA simulations, the next generation of the MUFASA cosmological galaxy formation simulations run with GIZMO's meshless finite mass hydrodynamics. SIMBA includes updates to MUFASA's sub-resolution star formation and feedback prescriptions, and introduces black hole growth via the torque-limited accretion model of Angl{\'e}s-Alc{\'a}zar et al. from cold gas and Bondi accretion from hot gas, along with black hole feedback via kinetic bipolar outflows and X-ray energy. Ejection velocities are taken to be \{? \} 10{\textasciicircum}3 km s{\textasciicircum}\{-1\} at high Eddington ratios, increasing to \{? \} 8000 km s{\textasciicircum}\{-1\} at Eddington ratios below 2 per cent, with a constant momentum input of 20L/c. SIMBA further includes an on-the-fly dust production, growth, and destruction model. Our SIMBA run with (100h{\textasciicircum}\{-1\} Mpc){\textasciicircum}3 and 1024{\textless}SUP{\textgreater}3{\textless}/SUP{\textgreater} gas elements reproduces numerous observables, including galaxy stellar mass functions at z = 0-6, the stellar mass-star formation rate main sequence, H I and H{\textless}SUB{\textgreater}2{\textless}/SUB{\textgreater} fractions, the mass-metallicity relation at z ? 0, 2, star-forming galaxy sizes, hot gas fractions in massive haloes, and z = 0 galaxy dust properties. However, SIMBA also yields an insufficiently sharp truncation of the z = 0 mass function, and too-large sizes for low-mass quenched galaxies. We show that SIMBA's jet feedback is primarily responsible for quenching massive galaxies.},
	language = {en},
	number = {2},
	urldate = {2019-05-06},
	journal = {\mnras},
	author = {Dav{\'e}, Romeel and Angl{\'e}s-Alc{\'a}zar, Daniel and Narayanan, Desika and Li, Qi and Rafieferantsoa, Mika H. and Appleby, Sarah},
	month = jun,
	year = {2019},
	pages = {2827}
}

@article{katsianis_evolving_2019,
	title = {An evolving and mass dependent \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{{sSFR}\}\}\}\$-\$\{{\textbackslash}rm {M}\_\{{\textbackslash}star\}\}\$ relation for galaxies},
	url = {http://arxiv.org/abs/1905.02023},
	abstract = {The scatter (\$\{{\textbackslash}rm{\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$) of the specific star formation rates (sSFRs) of galaxies is a measure of the diversity in their star formation histories (SFHs) at a given mass. In this paper we employ the EAGLE simulations to study the dependence of the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$ of galaxies on stellar mass (\$\{{\textbackslash}rm M\_\{{\textbackslash}star\}\}\$) through the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$-\$\{{\textbackslash}rm M\_\{{\textbackslash}star\}\}\$ relation in \$ \{{\textbackslash}rm z {\textbackslash}sim 0-4\}\$. We find that the relation evolves with time, with the dispersion depending on both stellar mass and redshift. The models point to an evolving U-shape form for the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$-\$\{{\textbackslash}rm M\_\{{\textbackslash}star\}\}\$ relation with the scatter being minimal at a characteristic mass \$M{\textasciicircum}\{{\textbackslash}star\}\$ of \$\{{\textbackslash}rm 10{\textasciicircum}\{9.5\}\}\$ \$\{{\textbackslash}rm M\_\{{\textbackslash}odot\}\}\$ and increasing both at lower and higher masses. This implication is that the diversity of SFHs increases towards both at the low- and high-mass ends. We find that active galactic nuclei feedback is important for increasing the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$ for high mass objects. On the other hand, we suggest that SNe feedback increases the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$ of galaxies at the low-mass end. We also find that excluding galaxies that have experienced recent mergers does not significantly affect the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$-\$\{{\textbackslash}rm M\_\{{\textbackslash}star\}\}\$ relation. Furthermore, we employ the combination of the EAGLE simulations with the radiative transfer code SKIRT to evaluate the effect of SFR/stellar mass diagnostics in the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$-\$\{{\textbackslash}rm M\_\{{\textbackslash}star\}\}\$ relation and find that the \$\{{\textbackslash}rm SFR/M\_\{{\textbackslash}star\}\}\$ methodologies (e.g. SED fitting, UV+IR, UV+IRX-\${\textbackslash}beta\$) widely used in the literature to obtain intrinsic properties of galaxies have a large effect on the derived shape and normalization of the \$\{{\textbackslash}rm {\textbackslash}sigma\_\{{\textbackslash}text\{sSFR\}\}\}\$-\$\{{\textbackslash}rm M\_\{{\textbackslash}star\}\}\$ relation.},
	urldate = {2019-05-07},
	journal = {arXiv:1905.02023 [astro-ph]},
	author = {Katsianis, Antonios and Zheng, Xianzhong and Gonzalez, Valentino and Blanc, Guillermo and Lagos, Claudia del P. and Davies, Luke J. M. and Camps, Peter and Tr{\v c}ka, Ana and Baes, Maarten and Schaye, Joop and Trayford, James W. and Theuns, Tom and Stalevski, Marko},
	month = may,
	year = {2019},
	note = {arXiv: 1905.02023},
	keywords = {Astrophysics - Astrophysics of Galaxies}
}



