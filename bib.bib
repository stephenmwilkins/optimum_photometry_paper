@article{lee_turnover_2015,
	title = {A {Turnover} in the {Galaxy} {Main} {Sequence} of {Star} {Formation} at {M} * 1010 {M} ? for {Redshifts} z {\textless} 1.3},
	volume = {801},
	issn = {0004-637X},
	url = {http://stacks.iop.org/0004-637X/801/i=2/a=80},
	doi = {10.1088/0004-637X/801/2/80},
	abstract = {The relationship between galaxy star formation rates (SFRs) and stellar masses ( M * ) is reexamined using a mass-selected sample of 62,000 star-forming galaxies at z <= 1.3 in the COSMOS 2 deg 2 field. Using new far-infrared photometry from Herschel -PACS and SPIRE and Spitzer -MIPS 24 $\mu$m, along with derived infrared luminosities from the NRK method based on galaxies' locations in the restframe color-color diagram (NUV {\textendash} r ) versus ( r {\textendash} K ), we are able to more accurately determine total SFRs for our complete sample. At all redshifts, the relationship between median SFR and M * follows a power law at low stellar masses, and flattens to nearly constant SFR at high stellar masses. We describe a new parameterization that provides the best fit to the main sequence and characterizes the low mass power-law slope, turnover mass, and overall scaling. The turnover in the main sequence occurs at a characteristic mass of about M 0 10 10 M ? at all redshifts. The low mass power-law slope ranges from 0.9-1.3 and the overall scaling rises in SFR as a function of (1 + z ) 4.12 {\textpm} 0.10 . A broken power-law fit below and above the turnover mass gives relationships of \#\#IMG\#\# [http://ej.iop.org/images/0004-637X/801/2/80/apj508025ieqn1.gif] \${\textbackslash}rm SFR {\textbackslash}propto M\_*{\textasciicircum}0.88 {\textbackslash}pm 0.06\$ below the turnover mass and \#\#IMG\#\# [http://ej.iop.org/images/0004-637X/801/2/80/apj508025ieqn2.gif] \${\textbackslash}rm SFR {\textbackslash}propto M\_*{\textasciicircum}0.27 {\textbackslash}pm 0.04\$ above the turnover mass. Galaxies more massive than M * \#\#IMG\#\# [http://ej.iop.org/icons/Entities/gsim.gif] gsim 10 10 M ? have a much lower average specific star formation rate (sSFR) than would be expected by simply extrapolating the traditional linear fit to the main sequence found for less massive galaxies.},
	language = {en},
	number = {2},
	urldate = {2018-02-01},
	journal = {ApJ},
	author = {Lee, Nicholas and Sanders, D. B. and Casey, Caitlin M. and Toft, Sune and Scoville, N. Z. and Hung, Chao-Ling and Floc'h, Emeric Le and Ilbert, Olivier and Zahid, H. Jabran and Aussel, Herv{\'e} and Capak, Peter and Kartaltepe, Jeyhan S. and Kewley, Lisa J. and Li, Yanxia and Schawinski, Kevin and Sheth, Kartik and Xiao, Quanbao},
	year = {2015},
	pages = {80}
}

@article{tasca_evolving_2015,
	title = {The evolving star formation rate: {M}* relation and {sSFR} since z ? 5 from the {VUDS} spectroscopic survey},
	volume = {581},
	copyright = {{\textcopyright} ESO, 2015},
	issn = {0004-6361, 1432-0746},
	shorttitle = {The evolving star formation rate},
	url = {https://www.aanda.org/articles/aa/abs/2015/09/aa25379-14/aa25379-14.html},
	doi = {10.1051/0004-6361/201425379},
	abstract = {We study the evolution of the star formation rate (SFR) {\textendash} stellar mass ({\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}*{\textless}i/{\textgreater}{\textless}sub/{\textgreater}) relation and specific star formation rate (sSFR) of star-forming galaxies (SFGs) since a redshift {\textless}i{\textgreater}z{\textless}i/{\textgreater} ? 5.5 using 2435 (4531) galaxies with highly reliable spectroscopic redshifts in the VIMOS Ultra-Deep Survey (VUDS). It is the first time that these relations can be followed over such a large redshift range from a single homogeneously selected sample of galaxies with spectroscopic redshifts. The log ({\textless}i{\textgreater}SFR{\textless}i/{\textgreater}) - log ({\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}*{\textless}i/{\textgreater}{\textless}sub/{\textgreater}) relation for SFGs remains roughly linear all the way up to {\textless}i{\textgreater}z{\textless}i/{\textgreater} = 5, but the SFR steadily increases at fixed mass with increasing redshift. We find that for stellar masses {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}*{\textless}i/{\textgreater}{\textless}sub/{\textgreater} >= 3.2 {\texttimes} 10{\textless}sup{\textgreater}9{\textless}sup/{\textgreater}{\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}?{\textless}sub/{\textgreater} the SFR increases by a factor of {\textasciitilde}13 between {\textless}i{\textgreater}z{\textless}i/{\textgreater} = 0.4 and {\textless}i{\textgreater}z{\textless}i/{\textgreater} = 2.3. Weextend this relation up to {\textless}i{\textgreater}z{\textless}i/{\textgreater} = 5, finding an additional increase in SFR by a factor of 1.7 from {\textless}i{\textgreater}z{\textless}i/{\textgreater} = 2.3 to {\textless}i{\textgreater}z{\textless}i/{\textgreater} = 4.8 for masses {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}*{\textless}i/{\textgreater}{\textless}sub/{\textgreater} >= 10{\textless}sup{\textgreater}10{\textless}sup/{\textgreater}{\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}?{\textless}sub/{\textgreater}. We observe a turn-off in the {\textless}i{\textgreater}SFR{\textless}i/{\textgreater}{\textendash}{\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}*{\textless}i/{\textgreater}{\textless}sub/{\textgreater} relation at the highest mass end up to a redshift {\textless}i{\textgreater}z{\textless}i/{\textgreater} {\textasciitilde} 3.5. We interpret this turn-off as the signature of a strong on-going quenching mechanism and rapid mass growth. The sSFR increases strongly up to {\textless}i{\textgreater}z{\textless}i/{\textgreater} {\textasciitilde} 2, but it grows much less rapidly in 2 {\textless}{\textless}i{\textgreater}z{\textless}i/{\textgreater}{\textless} 5. We find that the shape of the sSFR evolution is not well reproduced by cold gas accretion-driven models or the latest hydrodynamical models. Below {\textless}i{\textgreater}z{\textless}i/{\textgreater} {\textasciitilde} 2 these models have a flatter evolution (1 + {\textless}i{\textgreater}z{\textless}i/{\textgreater}){\textless}sup{\textgreater}$\Phi${\textless}sup/{\textgreater} with $\Phi$ = 2 - 2.25 compared to the data which evolves more rapidly with $\Phi$ = 2.8 {\textpm} 0.2. Above {\textless}i{\textgreater}z{\textless}i/{\textgreater} {\textasciitilde} 2, the reverse is happening with the data evolving more slowly with $\Phi$ = 1.2 {\textpm} 0.1. The observed sSFR evolution over a large redshift range 0 {\textless}{\textless}i{\textgreater}z{\textless}i/{\textgreater}{\textless} 5 and our finding of a non-linear main sequence at high mass both indicate that the evolution of SFR and {\textless}i{\textgreater}M{\textless}i/{\textgreater}{\textless}sub{\textgreater}{\textless}i{\textgreater}*{\textless}i/{\textgreater}{\textless}sub/{\textgreater} is not solely driven by gas accretion. The results presented in this paper emphasize the need to invoke a more complex mix of physical processes including major and minor merging to further understand the co-evolution of the SFR and stellar mass growth.},
	language = {en},
	urldate = {2018-02-05},
	journal = {A\&A},
	author = {Tasca, L. a. M. and F{\`e}vre, O. Le and Hathi, N. P. and Schaerer, D. and Ilbert, O. and Zamorani, G. and Lemaux, B. C. and Cassata, P. and Garilli, B. and Brun, V. Le and Maccagni, D. and Pentericci, L. and Thomas, R. and Vanzella, E. and Zucca, E. and Amorin, R. and Bardelli, S. and Cassar{\`a}, L. P. and Castellano, M. and Cimatti, A. and Cucciati, O. and Durkalec, A. and Fontana, A. and Giavalisco, M. and Grazian, A. and Paltani, S. and Ribeiro, B. and Scodeggio, M. and Sommariva, V. and Talia, M. and Tresse, L. and Vergani, D. and Capak, P. and Charlot, S. and Contini, T. and Torre, S. de la and Dunlop, J. and Fotopoulou, S. and Koekemoer, A. and L{\'o}pez-Sanjuan, C. and Mellier, Y. and Pforr, J. and Salvato, M. and Scoville, N. and Taniguchi, Y. and Wang, P. W.},
	year = {2015},
	pages = {A54}
}

@article{leja_reconciling_2015,
	title = {Reconciling the {Observed} {Star}-forming {Sequence} with the {Observed} {Stellar} {Mass} {Function}},
	volume = {798},
	issn = {0004-637X},
	url = {http://stacks.iop.org/0004-637X/798/i=2/a=115},
	doi = {10.1088/0004-637X/798/2/115},
	abstract = {We examine the connection between the observed star-forming sequence (SFR \#\#IMG\#\# [http://ej.iop.org/icons/Entities/vprop.gif] vprop M $\alpha$ ) and the observed evolution of the stellar mass function in the range 0.2 {\textless} z {\textless} 2.5. We find that the star-forming sequence cannot have a slope $\alpha$ \#\#IMG\#\# [http://ej.iop.org/icons/Entities/lsim.gif] lsim 0.9 at all masses and redshifts because this would result in a much higher number density at 10 {\textless} log ( M / M ? ) {\textless} 11 by z = 1 than is observed. We show that a transition in the slope of the star-forming sequence, such that $\alpha$ = 1 at log ( M / M ? ) 10.5, greatly improves agreement with the evolution of the stellar mass function. We then derive a star-forming sequence that reproduces the evolution of the mass function by design. This star-forming sequence is also well described by a broken power law, with a shallow slope at high masses and a steep slope at low masses. At z = 2, it is offset by 0.3 dex from the observed star-forming sequence, consistent with the mild disagreement between the cosmic star formation rate (SFR) and recent observations of the growth of the stellar mass density. It is unclear whether this problem stems from errors in stellar mass estimates, errors in SFRs, or other effects. We show that a mass-dependent slope is also seen in other self-consistent models of galaxy evolution, including semianalytical, hydrodynamical, and abundance-matching models. As part of the analysis, we demonstrate that neither mergers nor hidden low-mass quiescent galaxies are likely to reconcile the evolution of the mass function and the star-forming sequence. These results are supported by observations from Whitaker et al.},
	language = {en},
	number = {2},
	urldate = {2018-02-05},
	journal = {ApJ},
	author = {Leja, Joel and Dokkum, Pieter G. van and Franx, Marijn and Whitaker, Katherine E.},
	year = {2015},
	pages = {115}
}

@article{yung_semi-analytic_2018,
	title = {Semi-analytic forecasts for {JWST} - {I}. {UV} luminosity functions at z = 4 - 10},
	url = {http://arxiv.org/abs/1803.09761},
	abstract = {In anticipation of the upcoming deployment of the James Webb Space Telescope (JWST), we present high-redshift predictions by the well-established Santa Cruz semi-analytic model. We update the models by re-calibrating them after adopting cosmological parameters consistent with recent constraints from Planck. We provide predictions for rest-frame UV luminosity functions for galaxy populations over a wide range of \$M\_{\textbackslash}text\{UV\}\$ from \${\textbackslash}sim-6\$ to \${\textbackslash}sim-24\$ between \$z = 4 - 10\$. In addition, we present the corresponding predictions for observed-frame galaxy number counts in different redshift bins in the full set of NIRCam filters. We provide predictions of the quantitative effect on these observables of varying the physical recipes implemented in the models, such as the molecular gas depletion time (star formation efficiency) scalings or the scalings of outflow rates driven by stars and supernovae with galaxy circular velocity. Based on these results, we discuss what may be learned about the physical processes that shape galaxy formation from JWST observations of galaxy number densities at different intrinsic luminosities. All data tables for the results presented in this work are available at www.physics.rutgers.edu/{\textasciitilde}yung/JWST/.},
	urldate = {2018-04-16},
	journal = {arXiv:1803.09761 [astro-ph]},
	author = {Yung, L. Y. Aaron and Somerville, Rachel S. and Finkelstein, Steven L. and Popping, Gerg{\"o} and Dav{\'e}, Romeel},
	month = mar,
	year = {2018},
	note = {arXiv: 1803.09761},
	keywords = {Astrophysics - Astrophysics of Galaxies}
}

@article{vogelsberger_introducing_2014,
	title = {Introducing the {Illustris} {Project}: simulating the coevolution of dark and visible matter in the {Universe}},
	volume = {444},
	issn = {0035-8711},
	shorttitle = {Introducing the {Illustris} {Project}},
	url = {http://adsabs.harvard.edu/abs/2014MNRAS.444.1518V},
	doi = {10.1093/mnras/stu1536},
	abstract = {We introduce the Illustris Project, a series of large-scale 
hydrodynamical simulations of galaxy formation. The highest resolution
simulation, Illustris-1, covers a volume of (106.5 Mpc)3, has
a dark mass resolution of 6.26 {\texttimes} 106 M?,
and an initial baryonic matter mass resolution of 1.26 {\texttimes}
106 M?. At z = 0 gravitational forces are
softened on scales of 710 pc, and the smallest hydrodynamical gas cells
have an extent of 48 pc. We follow the dynamical evolution of 2 {\texttimes}
18203 resolution elements and in addition passively evolve
18203 Monte Carlo tracer particles reaching a total particle
count of more than 18 billion. The galaxy formation model includes:
primordial and metal-line cooling with self-shielding corrections,
stellar evolution, stellar feedback, gas recycling, chemical enrichment,
supermassive black hole growth, and feedback from active galactic
nuclei. Here we describe the simulation suite, and contrast basic
predictions of our model for the present-day galaxy population with
observations of the local universe. At z = 0 our simulation volume
contains about 40 000 well-resolved galaxies covering a diverse range of
morphologies and colours including early-type, late-type and irregular
galaxies. The simulation reproduces reasonably well the cosmic star
formation rate density, the galaxy luminosity function, and baryon
conversion efficiency at z = 0. It also qualitatively captures the
impact of galaxy environment on the red fractions of galaxies. The
internal velocity structure of selected well-resolved disc galaxies
obeys the stellar and baryonic Tully-Fisher relation together with flat
circular velocity curves. In the well-resolved regime, the simulation
reproduces the observed mix of early-type and late-type galaxies. Our
model predicts a halo mass dependent impact of baryonic effects on the
halo mass function and the masses of haloes caused by feedback from
supernova and active galactic nuclei.},
	urldate = {2018-08-21},
	journal = {\mnras},
	author = {Vogelsberger, Mark and Genel, Shy and Springel, Volker and Torrey, Paul and Sijacki, Debora and Xu, Dandan and Snyder, Greg and Nelson, Dylan and Hernquist, Lars},
	month = oct,
	year = {2014},
	keywords = {cosmology: theory, methods: numerical},
	pages = {1518--1547}
}

